{
  "name": "@ben-ryder/jigsaw-react",
  "version": "0.10.0",
  "description": "A design system and component library for use in my personal projects.",
  "author": "Ben-Ryder",
  "license": "AGPL-3.0-only",
  "type": "module",
  "homepage": "https://github.com/Ben-Ryder/jigsaw#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ben-Ryder/jigsaw.git"
  },
  "bugs": {
    "url": "https://github.com/Ben-Ryder/jigsaw/issues"
  },
  "engines": {
    "node": "18.x"
  },
  "files": [
    "dist/fonts",
    "dist/css",
    "**/*.scss",
    "!**/*.development.scss",
    "!src/development.scss",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "start": "storybook dev -p 6006 --no-open",
    "build": "npm run build:storybook && npm run build:sass",
    "build:storybook": "storybook build -o build",
    "build:sass": "sass src/jigsaw.scss dist/css/jigsaw.css"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "lucide-react": "^0.108.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^7.0.0-beta.40",
    "@storybook/addon-essentials": "^7.0.0-beta.40",
    "@storybook/addon-interactions": "^7.0.0-beta.40",
    "@storybook/addon-links": "^7.0.0-beta.40",
    "@storybook/addons": "^7.0.0-beta.40",
    "@storybook/blocks": "^7.0.0-beta.40",
    "@storybook/react": "^7.0.0-beta.40",
    "@storybook/react-vite": "^7.0.0-beta.40",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/theming": "^7.0.0-beta.40",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "prop-types": "^15.8.1",
    "sass": "^1.57.1",
    "storybook": "^7.0.0-beta.40",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
